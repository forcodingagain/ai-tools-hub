{
    "name": "ai_tools_nextjs",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "dev": "cross-env NODE_OPTIONS='--max-old-space-size=2048' NODE_ENV=development next dev --turbo",
        "dev:turbo": "cross-env NODE_OPTIONS='--max-old-space-size=2048' NODE_ENV=development next dev --turbo",
        "dev:fast": "cross-env NODE_OPTIONS='--max-old-space-size=2048' next dev --turbo",
        "dev:optimized": "cross-env NODE_OPTIONS='--max-old-space-size=2048' NODE_ENV=development next dev --turbo",
        "dev:debug": "cross-env DEBUG=* NODE_OPTIONS='--max-old-space-size=2048' next dev --turbo",
        "dev:webpack": "cross-env NODE_OPTIONS='--max-old-space-size=2048' NODE_ENV=development next dev",
        "build": "next build",
        "build:turbo": "cross-env NODE_OPTIONS='--max-old-space-size=4096' next build --turbo",
        "build:analyze": "ANALYZE=true next build",
        "start": "next start",
        "lint": "next lint",
        "clean": "rm -rf .next && rm -rf node_modules/.cache",
        "db:check": "sqlite3 ai_tools.db 'SELECT COUNT(*) as tools FROM tools WHERE is_deleted = 0; SELECT COUNT(*) as categories FROM categories WHERE is_deleted = 0;'",
        "db:optimize": "sqlite3 ai_tools.db 'VACUUM; ANALYZE;'",
        "type-check": "tsc --noEmit"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "description": "AI工具导航门户 - 优化版",
    "dependencies": {
        "@ant-design/icons": "^6.1.0",
        "@ant-design/nextjs-registry": "^1.1.0",
        "ahooks": "^3.9.5",
        "antd": "^5.27.5",
        "better-sqlite3": "^12.4.1",
        "classnames": "^2.5.1",
        "lodash": "^4.17.21",
        "next": "^15.5.5",
        "react": "18.3.1",
        "react-dom": "18.3.1"
    },
    "devDependencies": {
        "@types/better-sqlite3": "^7.6.13",
        "@types/node": "24.7.2",
        "@types/react": "18.3.18",
        "autoprefixer": "^10.4.21",
        "cross-env": "^10.1.0",
        "postcss": "^8.5.6",
        "postcss-flexbugs-fixes": "^5.0.2",
        "postcss-loader": "^8.2.0",
        "postcss-preset-env": "^10.4.0",
        "typescript": "5.9.3"
    },
    "overrides": {
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "@types/react": "18.3.18"
    },
    "engines": {
        "node": ">=18.0.0",
        "react": "18.3.1"
    }
}
